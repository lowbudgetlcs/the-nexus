{
  "arrowParens": "avoid",
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 100,
  "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ],
  "quoteProps": "consistent",
  "semi": true,
  "svelteSortOrder": "options-scripts-markup-styles",
  "useTabs": false,
  "tailwindStylesheet": "./src/app.css"
}
